{
    "#page1": {
        "justifyContent": "space-between",
        "alignItems": "stretch",
        "paddingBottom": 20,
        "paddingLeft": 16,
        "paddingRight": 16
    },
    "#page1-lbl": {
        "backgroundColor": "rgba( 255, 255, 255, 0 )",
        "marginLeft": 20,
        "marginRight": 20,
        "alignSelf": "stretch",
        "font": {
            "size": 32,
            "bold": false,
            "italic": false,
            "family": "SFProText",
            "style": "Regular"
        },
        "multiline": true,
        "textAlign": "center",
        "textColor": "${mainTextColor}",
        "borderWidth": 2,
        "borderRadius": 2,
        "borderRadiusEdges": {
            "bottomRight": false,
            "topRight": false
        }
    },
    "#page1-label1": {
        "height": null,
        "width": null,
        "font": {
            "size": 36
        },
        "textAlign": "center"
    },
    "#page1-wrapper": {
        "flex": 1,
        "justifyContent": "center",
        "height": null,
        "width": null
    },
    "#page1-btnNext": {
        "alignSelf": "auto",
        "position": "relative",
        "width": null
    }
}